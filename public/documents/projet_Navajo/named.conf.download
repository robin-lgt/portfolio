zone "galaxy-swiss2.lan" {	
	type master;
	file "/etc/bind/db.galaxy-swiss2.lan";
	allow-update { key "rndc-key"; };
};

zone "0.16.172.in-addr.arpa" {
	type master;
	file "/etc/bind/db.172.16.0";
	allow-update { key "rndc-key"; };
};

zone "2.18.172.in-addr.arpa" {
	type master;
	file "/etc/bind/db.172.18.2";
	allow-update { key "rndc-key"; ];
};
